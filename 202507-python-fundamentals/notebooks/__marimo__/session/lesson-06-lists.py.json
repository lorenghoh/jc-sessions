      "code_hash": null,
      "outputs": [
        {
          "type": "data",
          "data": {
            "text/html": "<span class=\"markdown prose dark:prose-invert\"><h2 id=\"removing-items\">Removing items</h2>\n<span class=\"paragraph\">Just like adding items, there are two ways to remove an item from a list.</span>\n<ul>\n<li><code>pop()</code> if you want to remove an item at a specific index</li>\n<li><code>remove()</code> if you want to remove an item based on its value</li>\n</ul></span>"
          }
        }
      ],
      "console": []
    },
    {
